<launch>

	<!-- Set Control Type and Params -->
	<!--
	<arg name="control" default="direct" />
	<param name="control_type" type="str" value=$(arg control) />

	<arg name="num_agents" default="3">
	<param name="num_agents" type="int" value=$(arg num_agents) />
